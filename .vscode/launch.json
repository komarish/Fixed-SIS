{
  "name": "Launch blif.exe",
  "type": "cppdbg",
  "request": "launch",
  "program": "${workspaceFolder}/blif.exe",
  "externalConsole": true,
  "logging": {
    "moduleLoad": false,
    "trace": true
  },
  "showDisplayString": true,
  "configurations": [
    {
        "type": "gdb",
        "request": "launch",
        "name": "Launch Program",
        "target": "./blif.exe",
        "cwd": "${workspaceRoot}",
        "valuesFormatting": "parseText"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/marina/cont",
      "program": "/home/marina/cont/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}